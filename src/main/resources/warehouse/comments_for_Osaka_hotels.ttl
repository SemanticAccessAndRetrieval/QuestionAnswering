<urn:uuid:fabb1559-34a2-480f-98c4-0b9ae5f3f448>
        a            hip:review ;
        hip:hasDate  "09/04/2014" ;
        hip:hasText  "No wifi - N/A" .

<urn:uuid:34200484-a493-4f87-8814-adb107f22278>
